      "marks": 30,
      "questions": [
        {
          "id": "m1-q1",
          "subject": "Mathematics I",
          "type": "MCQ",
          "text": "Let f : ℝ → ℝ be defined by f(x) = x² - 3x + 6. Find the number of points where f(x) = f⁻¹(x) given x ≥ 1.5.",
          "options": [
            "0",
            "1",
            "2",
            "Infinite"
          ],
          "correctIndex": 0,
          "explanation": "Intersection must be on y=x. x²-4x+6=0 has D<0.",
          "year": "2021",
          "level": "Foundation",
          "marks": 2
        },
        {
          "id": "m1-q2",
          "subject": "Mathematics I",
          "type": "NAT",
          "text": "If A = {1,2,3} and B = {3,4,5}, find n(A ∪ B).",
          "options": [],
          "correctValue": "5",
          "explanation": "A ∪ B = {1,2,3,4,5}. Size is 5.",
          "year": "2021",
          "level": "Foundation",
          "marks": 2
        },
        {
          "id": "m1-q3",
          "subject": "Mathematics I",
          "type": "MSQ",
          "text": "Which of the following are functions?",
          "options": [
            "f(x) = ±x",
            "f(x) = x²",
            "x = y²",
            "f(x) = sin(x)"
          ],
          "correctIndices": [
            1,
            3
          ],
          "explanation": "One input must have exactly one output.",
          "year": "2021",
          "level": "Foundation",
          "marks": 3
        },
        {
          "id": "m1-q4",
          "subject": "Mathematics I",
          "type": "MCQ",
          "text": "Which of the following is the domain of f(x) = log(x-1)?",
          "options": [
            "(1, ∞)",
            "[1, ∞)",
            "(-∞, 1)",
            "(-∞, 1]"
          ],
          "correctIndex": 0,
          "explanation": "Log is defined for positive arguments, so x-1 > 0 => x > 1.",
          "year": "2021",
          "level": "Foundation",
          "marks": 2
        },
        {
          "id": "m1-q5",
          "subject": "Mathematics I",
          "type": "MCQ",
          "text": "If f(x) = 2x + 1, find f(f(x)).",
          "options": [
            "4x + 3",
            "4x + 1",
            "2x + 2",
            "2x + 3"
          ],
          "correctIndex": 0,
          "explanation": "f(2x+1) = 2(2x+1) + 1 = 4x + 2 + 1 = 4x + 3.",
          "year": "2021",
          "level": "Foundation",
          "marks": 2
        }
      ]
    },
